{
    "name": "rabol/livewire-calendar",
    "description": "Laravel Livewire calendar component",
    "keywords": [
        "asantibanez",
        "livewire-calendar"
    ],
    "homepage": "https://github.com/asantibanez/livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rabol\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabol\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rabol\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "Rabol\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
